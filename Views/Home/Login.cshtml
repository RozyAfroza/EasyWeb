@{ ViewBag.Title = "Index";    Layout = "~/Views/Shared/_Layout.cshtml";}<section id="contact" style="top:30px;" class="contact">    <div class="container" data-aos="fade-up">        <div class="row" data-aos="fade-up" data-aos-delay="100">            <div class="col-lg-6">                <h2 class="text-center"> Login </h2>                <form  method="post" role="form" class="php-email-form">                                           <div class="form-group">                            <label>Username</label>                            <input type="text" name="userName" class="form-control"                            id="name" placeholder="Username" required>                        </div>                        <div class="form-group">                            <label>Password</label>                            <input type="password" class="form-control"                            name="password" id="password" placeholder="Password" required>                        </div>                                                         <div class="text-center"><button class=" btn btn-secondary" type="submit">Login</button></div>                    <a href="@Url.Action("Admin")">go to admin</a>                </form>            </div>        </div>    </div></section><!-- End Contact Section -->